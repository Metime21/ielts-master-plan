{
  "rewrites": [
    {
      "source": "/api/gemini-proxy",
      "destination": "/api/gemini-proxy/index.ts"
    },
    {
      "source": "/api/gemini-proxy/",
      "destination": "/api/gemini-proxy/index.ts"
    }
  ],
  "builds": [
    {
      "src": "/api/gemini-proxy/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "/api/gemini-proxy/index.ts": {
      "maxDuration": 60
    }
  }
}
